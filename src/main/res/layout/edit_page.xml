        android:layout_below="@+id/shortSeconds"
        android:layout_alignLeft="@+id/shortSeconds"
        android:layout_alignStart="@+id/shortSeconds"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Long Break Length:"
        android:id="@+id/textView14"

        android:layout_below="@+id/textView12"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/longHours"
        android:layout_below="@+id/textView14"
        android:layout_toLeftOf="@+id/longMinutes"
        android:layout_toStartOf="@+id/longMinutes"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/longMinutes"
        android:layout_alignTop="@+id/longHours"
        android:layout_centerHorizontal="true"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/longSeconds"
        android:layout_alignTop="@+id/longMinutes"
