        android:gravity="center_horizontal"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignLeft="@+id/sprintMinutes"
        android:layout_alignStart="@+id/sprintMinutes"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Seconds"
        android:id="@+id/textView9"
        android:gravity="center_horizontal"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignLeft="@+id/sprintSeconds"
        android:layout_alignStart="@+id/sprintSeconds"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Short Break Length:"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/shortHours"
        android:layout_below="@+id/textView10"
        android:layout_toLeftOf="@+id/shortMinutes"
        android:layout_toStartOf="@+id/shortMinutes"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/shortMinutes"
        android:layout_below="@+id/textView10"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/shortSeconds"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Hours"
        android:id="@+id/textView11"
        android:gravity="center_horizontal"
        android:layout_below="@+id/shortHours"
        android:layout_alignLeft="@+id/shortHours"
        android:layout_alignStart="@+id/shortHours"
        android:textColor="#eee9e9"/>

    <TextView
