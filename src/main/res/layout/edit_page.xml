<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#004080"
  >

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Total Time:"
        android:id="@+id/textView"
        android:gravity="center_vertical"
        android:layout_marginTop="82dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/totalHours"
        android:layout_below="@+id/textView"
        android:layout_toLeftOf="@+id/totalMinutes"
        android:layout_toStartOf="@+id/totalMinutes"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/totalMinutes"
        android:layout_below="@+id/textView"
        android:gravity="center_vertical"
        android:layout_centerHorizontal="true"
        android:textColor="#eee9e9"/>
  <!--  android:layout_alignLeft="@+id/textView"
    android:layout_alignStart="@+id/textView" -->
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/totalSeconds"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/totalMinutes"
        android:layout_toEndOf="@+id/totalMinutes"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Hours"
        android:id="@+id/textView3"
        android:layout_below="@+id/totalHours"
        android:layout_alignLeft="@+id/totalHours"
        android:layout_alignStart="@+id/totalHours"
        android:gravity="center_horizontal"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Minutes"
        android:id="@+id/textView4"
        android:gravity="center_horizontal"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignLeft="@+id/totalMinutes"
        android:layout_alignStart="@+id/totalMinutes"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Seconds"
        android:id="@+id/textView5"
        android:gravity="center_horizontal"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignLeft="@+id/totalSeconds"
        android:layout_alignStart="@+id/totalSeconds"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Sprint Length:"
        android:id="@+id/textView6"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/sprintHours"
        android:layout_below="@+id/textView6"
        android:layout_toLeftOf="@+id/sprintMinutes"
        android:layout_toStartOf="@+id/sprintMinutes"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/sprintMinutes"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:gravity="center_vertical"
        android:textColor="#eee9e9"/>

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/sprintSeconds"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:textColor="#eee9e9"/>

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Hours"
        android:id="@+id/textView7"
        android:gravity="center_horizontal"
        android:layout_below="@+id/sprintHours"
